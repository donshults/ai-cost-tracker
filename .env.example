# Application
ENVIRONMENT=development
DEBUG=true
API_KEY=your_internal_api_key

# Anthropic
ANTHROPIC_API_KEY=your_anthropic_key

# Database (Neon PostgreSQL)
DATABASE_URL=postgresql+asyncpg://user:pass@host/db?sslmode=require

# Context Vault
CONTEXT_VAULT_API_KEY=your_cv_key
CONTEXT_VAULT_URL=https://context-vault.callteksupport.net
CONTEXT_VAULT_WORKSPACE=openclaw_default

# Collection Settings
COLLECTION_INTERVAL_HOURS=6
COLLECTION_ENABLED=true

# Project Mapping (JSON or use database)
# PROJECT_PATTERNS={"peakpaws-*": "PeakPaws", "kanban-*": "Kanban Board"}
